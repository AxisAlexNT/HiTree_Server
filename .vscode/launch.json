  "version": "0.2.0",
  "configurations": [
    {
      "name": "HiCT server from hict source",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "PYTHONPATH": "$PYTHONPATH:../hict/",
        "FLASK_APP": "hict_server/api_controller/dev_demo_server.py",
        "FLASK_ENV": "development"
      },
      "args": [
        "run",
        "--no-debugger"
      ],
      "jinja": true,
      "justMyCode": false
    },
